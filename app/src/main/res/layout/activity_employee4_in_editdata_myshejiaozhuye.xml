<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <include layout="@layout/actionbar" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="10dp"
        android:text="展示我的社交主页" />

    <EditText
        android:id="@+id/employee4_in_editdata_myshijiaozhuye_website"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/edittxt_clip5dp"
        android:hint="请输入网址" />

    <Button
        android:id="@+id/employee4_in_editdata_myshijiaozhuye_save"
        style="@style/Normal_Button_blue"
        android:layout_width="match_parent"
        android:layout_height="@dimen/btn_height"
        android:layout_gravity="center"
        android:layout_margin="@dimen/btn_marginLeft"
        android:text="保存" >
    </Button>

</LinearLayout>